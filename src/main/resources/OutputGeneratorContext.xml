<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        <bean id = "outputhelper" class="loosely_coupled.OutputHelper">
            <property name="output" ref="Jsonoutputgenerator"></property>
         </bean>
        <bean id = "Csvoutputgenerator" class="loosely_coupled.CsvOutputGenerator"></bean>
        <bean id = "Jsonoutputgenerator" class="loosely_coupled.JsonOutputGenerator"></bean>
</beans>